<?xml version="1.0" encoding="UTF-8"?>
<schema elementFormDefault="qualified"
	targetNamespace="http://api.callfire.com/data"
	xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://api.callfire.com/data"
	xmlns:ann="http://api.callfire.com/annotations"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	jaxb:version="2.0">

	<import namespace="http://api.callfire.com/annotations" schemaLocation="callfire-ann.xsd" />

	<!-- Simple Types -->

	<simpleType name="PhoneDigit">
		<restriction base="string">
			<pattern value="[0-9#\*]" />
		</restriction>
	</simpleType>

	<simpleType name="PhoneNumber">
		<restriction base="string">
			<pattern value="\+?[0-9]+" />
		</restriction>
	</simpleType>

	<simpleType name="PhoneNumberList">
		<list itemType="tns:PhoneNumber" />
	</simpleType>

	<simpleType name="KeywordList">
		<list itemType="string" />
	</simpleType>
	
	<!-- Enumerated Types -->

	<simpleType name="ActionState">
		<restriction base="string">
			<enumeration value="READY" />
			<enumeration value="SELECTED" />
			<enumeration value="FINISHED" />
			<enumeration value="DNC" />
			<enumeration value="DUP" />
			<enumeration value="INVALID" />
			<enumeration value="TIMEOUT" />
		</restriction>
	</simpleType>

	<simpleType name="Result">
		<restriction base="string">
			<enumeration value="LA" />
			<enumeration value="AM" />
			<enumeration value="BUSY" />
			<enumeration value="DNC" />
			<enumeration value="XFER" />
			<enumeration value="XFER_LEG" />
			<enumeration value="NO_ANS" />
			<enumeration value="UNDIALED" />

			<enumeration value="SENT" />
			<enumeration value="RECEIVED" />
			<enumeration value="DNT" />
			<enumeration value="TOO_BIG" />

			<enumeration value="INTERNAL_ERROR" />
			<enumeration value="CARRIER_ERROR" />
			<enumeration value="CARRIER_TEMP_ERROR" />
		</restriction>
	</simpleType>

	<simpleType name="BroadcastType">
		<restriction base="string">
			<enumeration value="VOICE" />
			<enumeration value="IVR" />
			<enumeration value="TEXT" />
		</restriction>
	</simpleType>

	<simpleType name="InboundType">
		<restriction base="string">
			<enumeration value="TRACKING" />
			<enumeration value="IVR" />
		</restriction>
	</simpleType>

	<simpleType name="DayOfWeek">
		<restriction base="string">
			<enumeration value="SUNDAY" />
			<enumeration value="MONDAY" />
			<enumeration value="TUESDAY" />
			<enumeration value="WEDNESDAY" />
			<enumeration value="THURSDAY" />
			<enumeration value="FRIDAY" />
			<enumeration value="SATURDAY" />
		</restriction>
	</simpleType>

	<simpleType name="BroadcastStatus">
		<restriction base="string">
			<enumeration value="START_PENDING" />
			<enumeration value="RUNNING" />
			<enumeration value="STOPPED" />
			<enumeration value="FINISHED" />
			<enumeration value="ARCHIVED" />
		</restriction>
	</simpleType>

	<simpleType name="BatchStatus">
		<restriction base="string">
			<enumeration value="NEW" />
			<enumeration value="VALIDATING" />
			<enumeration value="ERRORS" />
			<enumeration value="SOURCE_ERROR" />
			<enumeration value="ACTIVE" />
		</restriction>
	</simpleType>

	<simpleType name="AnsweringMachineConfig">
		<restriction base="string">
			<enumeration value="AM_ONLY" />
			<enumeration value="AM_AND_LIVE" />
			<enumeration value="LIVE_WITH_AMD" />
			<enumeration value="LIVE_IMMEDIATE" />
		</restriction>
	</simpleType>

	<simpleType name="BigMessageStrategy">
		<restriction base="string">
			<enumeration value="SEND_MULTIPLE" />
			<enumeration value="DO_NOT_SEND" />
			<enumeration value="TRIM" />
		</restriction>
	</simpleType>

	<simpleType name="NotificationFormat">
		<restriction base="string">
			<enumeration value="XML" />
			<enumeration value="JSON" />
			<enumeration value="SOAP" />
			<enumeration value="EMAIL" />
		</restriction>
	</simpleType>

	<simpleType name="SubscriptionTriggerEvent">
		<restriction base="string">
			<enumeration value="UNDEFINED_EVENT" />
			<enumeration value="INBOUND_CALL_FINISHED" />
			<enumeration value="INBOUND_TEXT_FINISHED" />
			<enumeration value="OUTBOUND_CALL_FINISHED" />
			<enumeration value="OUTBOUND_TEXT_FINISHED" />
			<enumeration value="CAMPAIGN_STARTED" />
			<enumeration value="CAMPAIGN_STOPPED" />
			<enumeration value="CAMPAIGN_FINISHED" />
		</restriction>
	</simpleType>

	<simpleType name="NumberStatus">
		<restriction base="string">
			<enumeration value="PENDING" />
			<enumeration value="ACTIVE" />
			<enumeration value="RELEASED" />
			<enumeration value="UNAVAILABLE" />
		</restriction>
	</simpleType>

	<simpleType name="NumberFeature">
		<restriction base="string">
			<enumeration value="UNSUPPORTED" />
			<enumeration value="PENDING" />
			<enumeration value="DISABLED" />
			<enumeration value="ENABLED" />
		</restriction>
	</simpleType>

	<simpleType name="OrderStatus">
		<restriction base="string">
			<enumeration value="NEW" />
			<enumeration value="PROCESSING" />
			<enumeration value="FINISHED" />
			<enumeration value="ERRORED" />
			<enumeration value="VOID" />
		</restriction>
	</simpleType>

	<simpleType name="SoundStatus">
		<restriction base="string">
			<enumeration value="PENDING">
				<annotation>
					<documentation>
						<ann:summary>Sound is being created/processed, and is not ready for use.</ann:summary>
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="ACTIVE">
				<annotation>
					<documentation>
						<ann:summary>Sound is available for download/use.</ann:summary>
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="FAILED">
				<annotation>
					<documentation>
						<ann:summary>Sound creation failed and is not recoverable.</ann:summary>
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="ARCHIVED">
				<annotation>
					<documentation>
						<ann:summary>Sound has been archived and will be excluded from query results.</ann:summary>
					</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

	<simpleType name="Voice">
		<restriction base="string">
			<enumeration value="MALE1" />
			<enumeration value="FEMALE1" />
			<enumeration value="FEMALE2" />
			<enumeration value="SPANISH1" />
		</restriction>
	</simpleType>

	<!--Without it xsl transformation failed-->
	<simpleType name="TTSText">
		<restriction base="string"/>
	</simpleType>

	<!-- Complex Types -->

	<complexType name="TextToSpeech">
		<simpleContent>
			<!--if use base="string" then xsl transformation failed when build callfire-api-->
			<extension base="tns:TTSText">
				<attribute name="Voice" type="tns:Voice" use="optional" default="FEMALE1" />
			</extension>
		</simpleContent>
	</complexType>

	<complexType name="InboundConfig" abstract="true">
		<attribute name="id" type="long" />
	</complexType>

	<complexType name="BroadcastConfig" abstract="true">
		<sequence>
			<element name="Created" type="dateTime" minOccurs="0">
				<annotation ann:responseOnly="true" >
					<documentation>
						<ann:summary>DateTime Broadcast was created 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
						<ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="FromNumber" type="tns:PhoneNumber" minOccurs="0">
				<annotation ann:restParam="From" />
			</element>
			<element ref="tns:LocalTimeZoneRestriction" minOccurs="0" />
			<element name="RetryConfig" minOccurs="0">
				<annotation ann:sinceVersion="1.1">
					<documentation>
						<ann:summary>Retry logic for broadcast</ann:summary>
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element name="MaxAttempts" type="int" minOccurs="0"
							default="1">
							<annotation>
								<documentation>
									<ann:summary>Max attempts to retry broadcast (default: 1)</ann:summary>
									<ann:demoValue>1</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<element name="MinutesBetweenAttempts" minOccurs="0" type="int"
							default="60">
							<annotation>
								<documentation>
									<ann:summary>Minutes between broadcast attempts (default: 60)</ann:summary>
									<ann:demoValue>60</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<element name="RetryResults">
							<annotation>
								<documentation>
									<ann:demoValue>SENT RECEIVED</ann:demoValue>
								</documentation>
							</annotation>
							<simpleType>
								<list itemType="tns:Result" />
							</simpleType>
						</element>
					</sequence>
				</complexType>
			</element>
		</sequence>
		<attribute name="id" type="long" />
	</complexType>

	<complexType name="ToNumber">
		<simpleContent>
			<extension base="tns:PhoneNumber">
				<attribute name="ClientData" type="string" />
				<anyAttribute processContents="lax" />
			</extension>
		</simpleContent>
	</complexType>
	
	<!-- abstract so no reason to annotate -->
	<complexType name="ActionRecord" abstract="true">
		<sequence>
			<element name="Result" type="tns:Result" />
			<element name="FinishTime" type="dateTime">
				<annotation>
					<documentation>
						<ann:summary>DateTime action finished in 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]' format</ann:summary>
						<ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="BilledAmount" type="float">
				<annotation>
					<documentation>
						<ann:summary>Credits charged for call or text</ann:summary>
						<ann:demoValue>1.00</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="QuestionResponse" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>
						<ann:summary><![CDATA[Question / Response pair defined in IVR campaign with <a href="https://www.callfire.com/help/docs/callfire-xml/tags/stash">'stash'.</a>]]></ann:summary>
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element name="Question" type="string">
							<annotation>
								<documentation>
									<ann:summary>IVR stash 'varname'</ann:summary>
									<ann:demoValue>question1</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<element name="Response" type="string">
							<annotation>
								<documentation>
									<ann:summary>IVR stash value associated with 'varname'</ann:summary>
									<ann:demoValue>1</ann:demoValue>
								</documentation>
							</annotation>
						</element>
					</sequence>
				</complexType>
			</element>
		</sequence>
		<attribute name="id" type="long" use="required" />
	</complexType>

	<complexType name="Action">
		<sequence>
			<element name="FromNumber" type="tns:PhoneNumber">
				<annotation>
					<documentation>
						<ann:summary>E.164 11 digit number or short code</ann:summary>
						<ann:summary>67076</ann:summary>
					</documentation>
				</annotation>
			</element>
			<element name="ToNumber" type="tns:ToNumber">
				<annotation>
					<documentation>
						<ann:summary>List of E.164 11 digit numbers space seperated</ann:summary>
						<ann:demoValue>13105551212 18185551212</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="State" type="tns:ActionState">
				<annotation>
					<documentation>
						<ann:summary>Current State of Action</ann:summary>
						<ann:demoValue>FINISHED</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="BatchId" type="long" minOccurs="0">
				<annotation>
					<documentation>
						<ann:summary>Unique ID of Batch associated with Action</ann:summary>
						<ann:demoValue>123</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="BroadcastId" type="long" minOccurs="0">
				<annotation>
					<documentation>
						<ann:summary>Unique ID of Broadcast associated with Action</ann:summary>
						<ann:demoValue>123</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="ContactId" type="long">
				<annotation>
					<documentation>
						<ann:summary>Unique ID of Contact associated with Action</ann:summary>
						<ann:demoValue>123</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="Inbound" type="boolean">
				<annotation>
					<documentation>
						<ann:summary>Is inbound Action</ann:summary>
						<ann:demoValue>false</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="Created" type="dateTime">
				<annotation>
					<documentation>
						<ann:summary>DateTime Action was created 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
						<ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="Modified" type="dateTime">
				<annotation>
					<documentation>
						<ann:summary>DateTime Action was modified 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
						<ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
					</documentation>
				</annotation>
			</element>
			<element name="FinalResult" type="tns:Result" minOccurs="0">
				<annotation>
					<documentation>
						<ann:summary></ann:summary>
						<ann:demoValue>NO_ANS</ann:demoValue>
					</documentation>
				</annotation>
			</element>
		</sequence>
		<attribute name="id" type="long" use="required" >
			<annotation>
				<documentation>
					<ann:summary>Unique ID of action</ann:summary>
					<ann:demoValue>123</ann:demoValue>
				</documentation>
			</annotation>
		</attribute>
	</complexType>
	
	<!-- Broadcast Elements -->

	<element name="Broadcast">
		<annotation>
			<documentation>
				<ann:summary>A TEXT, IVR, or VOICE Broadcast</ann:summary>
			</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="Name" type="string" minOccurs="0">
					<annotation ann:requiredIn="POST">
						<documentation>
							<ann:summary>Name of Broadcast</ann:summary>
							<ann:demoValue>My Test Broadcast</ann:demoValue>
						</documentation>
					</annotation>
				</element>
				<element name="Status" type="tns:BroadcastStatus" minOccurs="0">
					<annotation ann:responseOnly="true">
						<documentation>
							<ann:summary>Status of Broadcast (response only)</ann:summary>
						</documentation>
					</annotation>
				</element>
				<element name="LastModified" type="dateTime" minOccurs="0">
					<annotation ann:responseOnly="true">
						<documentation>
							<ann:summary>Last Modified</ann:summary>
						</documentation>
					</annotation>
				</element>
				<element name="Type" type="tns:BroadcastType" minOccurs="0">
					<annotation>
						<documentation>
							<ann:summary>Type of Broadcast</ann:summary>
							<ann:demoValue>TEXT</ann:demoValue>
						</documentation>
					</annotation>
				</element>
				<choice>
					<element ref="tns:VoiceBroadcastConfig">
						<annotation>
							<documentation>
								<ann:summary>Configuration of a Voice Broadcast</ann:summary>
								<ann:condition field="Type" value="VOICE" />
							</documentation>
						</annotation>
					</element>
					<element ref="tns:TextBroadcastConfig">
						<annotation>
							<documentation>
								<ann:summary>Configuration of a Text Broadcast</ann:summary>
								<ann:condition field="Type" value="TEXT" />
							</documentation>
						</annotation>
					</element>
					<element ref="tns:IvrBroadcastConfig">
						<annotation>
							<documentation>
								<ann:summary>Configuration of an IVR Broadcast</ann:summary>
								<ann:condition field="Type" value="IVR" />
							</documentation>
						</annotation>
					</element>
				</choice>
			</sequence>
			<attribute name="id" type="long">
				<annotation>
					<documentation>
						<ann:summary>Unique ID of Broadcast</ann:summary>
						<ann:demoValue>1</ann:demoValue>
					</documentation>
				</annotation>
			</attribute>
		</complexType>
	</element>

	<element name="VoiceBroadcastConfig">
		<annotation>
			<documentation>
				<ann:summary>Configuration needed for a Voice Broadcast</ann:summary>
			</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="tns:BroadcastConfig">
					<sequence>
						<element name="AnsweringMachineConfig" type="tns:AnsweringMachineConfig" minOccurs="0">
							<annotation>
								<documentation>
									<ann:summary>Action to take if machine answers</ann:summary>
									<ann:demoValue>LIVE_IMMEDIATE</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<choice minOccurs="0">
							<element name="LiveSoundText" type="tns:TextToSpeech" />
							<element name="LiveSoundId" type="long">
								<annotation>
									<documentation>
										<ann:summary>ID of Sound to play if call answered by live person</ann:summary>
										<ann:demoValue>123</ann:demoValue>
									</documentation>
								</annotation>
							</element>
						</choice>
						<choice minOccurs="0">
							<element name="MachineSoundText" type="tns:TextToSpeech" />
							<element name="MachineSoundId" type="long">
								<annotation>
									<documentation>
										<ann:summary>ID of Sound to play if call answered by machine</ann:summary>
										<ann:demoValue>124</ann:demoValue>
									</documentation>
								</annotation>
							</element>
						</choice>
						<choice minOccurs="0">
							<element name="TransferSoundText" type="tns:TextToSpeech" />
							<element name="TransferSoundId" type="long">
								<annotation>
									<documentation>
										<ann:summary>ID of Sound to play if call transfered</ann:summary>
										<ann:demoValue>125</ann:demoValue>
									</documentation>
								</annotation>
							</element>
						</choice>
						<element name="TransferDigit" type="tns:PhoneDigit" minOccurs="0">
							<annotation>
								<documentation>
									<ann:summary>Phone digit call transfers on if pressed</ann:summary>
									<ann:demoValue>1</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<element name="TransferNumber" type="tns:PhoneNumber" minOccurs="0">
							<annotation>
								<documentation>
									<ann:summary>Number to transfer call to</ann:summary>
									<ann:demoValue>18185551212</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<choice minOccurs="0">
							<element name="DncSoundText" type="tns:TextToSpeech" />
							<element name="DncSoundId" type="long">
								<annotation>
									<documentation>
										<ann:summary>Do Not Call unique ID of sound</ann:summary>
										<ann:demoValue>12345</ann:demoValue>
									</documentation>
								</annotation>
							</element>
						</choice>
						<element name="DncDigit" type="tns:PhoneDigit" minOccurs="0">
							<annotation>
								<documentation>
									<ann:summary>Do Not Call Digit</ann:summary>
									<ann:demoValue>9</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<element name="MaxActiveTransfers" type="int" minOccurs="0">
							<annotation>
								<documentation>
									<ann:summary>Max Transfers</ann:summary>
									<ann:demoValue>1</ann:demoValue>
								</documentation>
							</annotation>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>

	<element name="TextBroadcastConfig">
		<annotation>
			<documentation>
				<ann:summary>Configuration needed for a Text Broadcast</ann:summary>
			</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="tns:BroadcastConfig">
					<sequence>
						<element name="Message" type="string" minOccurs="0">
							<annotation>
								<documentation>
									<ann:summary>160 char or less message to be sent in text
										broadcast. Use rented 'keyword' in message if need response
									</ann:summary>
									<ann:demoValue>use your phone to text YOUR_KEYWORD to 67076,
										and we'll send you special coupons that you can use in our
										store.</ann:demoValue>
								</documentation>
							</annotation>
						</element>
						<element name="BigMessageStrategy" type="tns:BigMessageStrategy"
							minOccurs="0" default="SEND_MULTIPLE">
							<annotation>
								<appinfo>
									<jaxb:property generateIsSetMethod="true" />
								</appinfo>
								<documentation>
									<ann:summary>Set strategy if message is over 160 chars (default: SEND_MULTIPLE)</ann:summary>
									<ann:demoValue>SEND_MULTIPLE</ann:demoValue>
								</documentation>
							</annotation>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>

	<element name="IvrBroadcastConfig">
		<annotation>
			<documentation>
				<ann:summary>Configuration needed for an IVR Broadcast</ann:summary>
			</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="tns:BroadcastConfig">
					<sequence>
						<element name="DialplanXml" type="string" minOccurs="0" >
							<annotation>
								<documentation>
									<ann:summary>IVR xml document describing dialplan</ann:summary>
								</documentation>
							</annotation>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>

	<element name="CallTrackingConfig">
		<complexType>
			<complexContent>
				<extension base="tns:InboundConfig">
					<sequence>
						<element name="TransferNumber" minOccurs="0">
							<simpleType>
								<list itemType="tns:PhoneNumber" />
							</simpleType>
						</element>
						<element name="Screen" type="boolean" minOccurs="0"
							default="false">
							<annotation>
								<appinfo>
									<jaxb:property generateIsSetMethod="true" />
								</appinfo>
							</annotation>
						</element>
						<element name="Record" type="boolean" minOccurs="0"
							default="false">
							<annotation>
								<appinfo>
									<jaxb:property generateIsSetMethod="true" />
								</appinfo>
							</annotation>
						</element>
						<element name="IntroSoundId" type="long" minOccurs="0" />
						<element name="WhisperSoundId" type="long" minOccurs="0" />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>

	<element name="IvrInboundConfig">
		<complexType>
