				<element name="TextFeature" type="tns:NumberFeature" minOccurs="0" />
				<element name="InboundCallConfigurationType" type="tns:InboundType" minOccurs="0" />
				<element name="InboundCallConfiguration" minOccurs="0">
					<complexType>
						<choice>
							<element ref="tns:CallTrackingConfig">
								<annotation>
									<documentation>
										<ann:condition field="InboundCallConfigurationType" value="TRACKING" />
									</documentation>
								</annotation>
							</element>
							<element ref="tns:IvrInboundConfig">
								<annotation>
									<documentation>
										<ann:condition field="InboundCallConfigurationType" value="IVR" />
									</documentation>
								</annotation>
							</element>
						</choice>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>

