<?xml version="1.0" encoding="UTF-8"?><!--callfire-data.xsd, 1.1-->
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://api.callfire.com/data"
        xmlns:ann="http://api.callfire.com/annotations" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
        elementFormDefault="qualified" targetNamespace="http://api.callfire.com/data" jaxb:version="2.0">
    <import namespace="http://api.callfire.com/annotations" schemaLocation="callfire-ann.xsd"/>
    <!-- Simple Types -->
    <simpleType name="PhoneDigit">
        <restriction base="string">
            <pattern value="[0-9#\*]"/>
        </restriction>
    </simpleType>
    <simpleType name="PhoneNumber">
        <restriction base="string">
            <pattern value="\+?[0-9]+"/>
        </restriction>
    </simpleType>
    <simpleType name="PhoneNumberList">
        <list itemType="tns:PhoneNumber"/>
    </simpleType>
    <simpleType name="KeywordList">
        <list itemType="string"/>
    </simpleType>
    <!-- Enumerated Types -->
    <simpleType name="ActionState">
        <restriction base="string">
            <enumeration value="READY"/>
            <enumeration value="SELECTED"/>
            <enumeration value="FINISHED"/>
            <enumeration value="DNC"/>
            <enumeration value="DUP"/>
            <enumeration value="INVALID"/>
            <enumeration value="TIMEOUT"/>
        </restriction>
    </simpleType>
    <simpleType name="Result">
        <restriction base="string">
            <enumeration value="LA"/>
            <enumeration value="AM"/>
            <enumeration value="BUSY"/>
            <enumeration value="DNC"/>
            <enumeration value="XFER"/>
            <enumeration value="XFER_LEG"/>
            <enumeration value="NO_ANS"/>
            <enumeration value="UNDIALED"/>
            <enumeration value="SENT"/>
            <enumeration value="RECEIVED"/>
            <enumeration value="DNT"/>
            <enumeration value="TOO_BIG"/>
            <enumeration value="INTERNAL_ERROR"/>
            <enumeration value="CARRIER_ERROR"/>
            <enumeration value="CARRIER_TEMP_ERROR"/>
        </restriction>
    </simpleType>
    <simpleType name="BroadcastType">
        <restriction base="string">
            <enumeration value="VOICE"/>
            <enumeration value="IVR"/>
            <enumeration value="TEXT"/>
        </restriction>
    </simpleType>
    <simpleType name="InboundType">
        <restriction base="string">
            <enumeration value="TRACKING"/>
            <enumeration value="IVR"/>
        </restriction>
    </simpleType>
    <simpleType name="DayOfWeek">
        <restriction base="string">
            <enumeration value="SUNDAY"/>
            <enumeration value="MONDAY"/>
            <enumeration value="TUESDAY"/>
            <enumeration value="WEDNESDAY"/>
            <enumeration value="THURSDAY"/>
            <enumeration value="FRIDAY"/>
            <enumeration value="SATURDAY"/>
        </restriction>
    </simpleType>
    <simpleType name="BroadcastStatus">
        <restriction base="string">
            <enumeration value="START_PENDING"/>
            <enumeration value="RUNNING"/>
            <enumeration value="STOPPED"/>
            <enumeration value="FINISHED"/>
            <enumeration value="ARCHIVED"/>
        </restriction>
    </simpleType>
    <simpleType name="BatchStatus">
        <restriction base="string">
            <enumeration value="NEW"/>
            <enumeration value="VALIDATING"/>
            <enumeration value="ERRORS"/>
            <enumeration value="SOURCE_ERROR"/>
            <enumeration value="ACTIVE"/>
        </restriction>
    </simpleType>
    <simpleType name="AnsweringMachineConfig">
        <restriction base="string">
            <enumeration value="AM_ONLY"/>
            <enumeration value="AM_AND_LIVE"/>
            <enumeration value="LIVE_WITH_AMD"/>
            <enumeration value="LIVE_IMMEDIATE"/>
        </restriction>
    </simpleType>
    <simpleType name="BigMessageStrategy">
        <restriction base="string">
            <enumeration value="SEND_MULTIPLE"/>
            <enumeration value="DO_NOT_SEND"/>
            <enumeration value="TRIM"/>
        </restriction>
    </simpleType>
    <simpleType name="NotificationFormat">
        <restriction base="string">
            <enumeration value="XML"/>
            <enumeration value="JSON"/>
            <enumeration value="SOAP"/>
            <enumeration value="EMAIL"/>
        </restriction>
    </simpleType>
    <simpleType name="SubscriptionTriggerEvent">
        <restriction base="string">
            <enumeration value="UNDEFINED_EVENT"/>
            <enumeration value="INBOUND_CALL_FINISHED"/>
            <enumeration value="INBOUND_TEXT_FINISHED"/>
            <enumeration value="OUTBOUND_CALL_FINISHED"/>
            <enumeration value="OUTBOUND_TEXT_FINISHED"/>
            <enumeration value="CAMPAIGN_STARTED"/>
            <enumeration value="CAMPAIGN_STOPPED"/>
            <enumeration value="CAMPAIGN_FINISHED"/>
        </restriction>
    </simpleType>
    <simpleType name="NumberStatus">
        <restriction base="string">
            <enumeration value="PENDING"/>
            <enumeration value="ACTIVE"/>
            <enumeration value="RELEASED"/>
            <enumeration value="UNAVAILABLE"/>
        </restriction>
    </simpleType>
    <simpleType name="NumberFeature">
        <restriction base="string">
            <enumeration value="UNSUPPORTED"/>
            <enumeration value="PENDING"/>
            <enumeration value="DISABLED"/>
            <enumeration value="ENABLED"/>
        </restriction>
    </simpleType>
    <simpleType name="OrderStatus">
        <restriction base="string">
            <enumeration value="NEW"/>
            <enumeration value="PROCESSING"/>
            <enumeration value="FINISHED"/>
            <enumeration value="ERRORED"/>
            <enumeration value="VOID"/>
        </restriction>
    </simpleType>
    <simpleType name="SoundStatus">
        <restriction base="string">
            <enumeration value="PENDING">
                <annotation>
                    <documentation>
                        <ann:summary>Sound is being created/processed, and is not ready for use.</ann:summary>
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="ACTIVE">
                <annotation>
                    <documentation>
                        <ann:summary>Sound is available for download/use.</ann:summary>
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="FAILED">
                <annotation>
                    <documentation>
                        <ann:summary>Sound creation failed and is not recoverable.</ann:summary>
                    </documentation>
                </annotation>
            </enumeration>
            <enumeration value="ARCHIVED">
                <annotation>
                    <documentation>
                        <ann:summary>Sound has been archived and will be excluded from query results.</ann:summary>
                    </documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <!-- Complex Types -->
    <complexType name="InboundConfig" abstract="true">
        <attribute name="id" type="long"/>
    </complexType>
    <complexType name="BroadcastConfig" abstract="true">
        <sequence>
            <element name="Created" type="dateTime" minOccurs="0">
                <annotation ann:responseOnly="true">
                    <documentation>
                        <ann:summary>DateTime Broadcast was created 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
                        <ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="FromNumber" type="tns:PhoneNumber" minOccurs="0">
                <annotation>
                    <xs:appinfo xmlns:xs="http://www.w3.org/2001/XMLSchema"
                                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                        <jaxb:bindings node="ancestor::xs:annotation/..">
                            <ann:annotate ann:requiredIn="POST" ann:restParam="From"/>
                        </jaxb:bindings>
                    </xs:appinfo>
                </annotation>
            </element>
            <element ref="tns:LocalTimeZoneRestriction" minOccurs="0"/>
            <element name="RetryConfig" minOccurs="0">
                <annotation ann:sinceVersion="1.1">
                    <documentation>
                        <ann:summary>Retry logic for broadcast</ann:summary>
                    </documentation>
                </annotation>
                <complexType>
                    <sequence>
                        <element name="MaxAttempts" type="int" minOccurs="0" default="1">
                            <annotation>
                                <documentation>
                                    <ann:summary>Max attempts to retry broadcast (default: 1)</ann:summary>
                                    <ann:demoValue>1</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="MinutesBetweenAttempts" minOccurs="0" type="int" default="60">
                            <annotation>
                                <documentation>
                                    <ann:summary>Minutes between broadcast attempts (default: 60)</ann:summary>
                                    <ann:demoValue>60</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="RetryResults">
                            <annotation>
                                <documentation>
                                    <ann:demoValue>SENT RECEIVED</ann:demoValue>
                                </documentation>
                            </annotation>
                            <simpleType>
                                <list itemType="tns:Result"/>
                            </simpleType>
                        </element>
                    </sequence>
                </complexType>
            </element>
        </sequence>
        <attribute name="id" type="long"/>
    </complexType>
    <complexType name="ToNumber">
        <simpleContent>
            <extension base="tns:PhoneNumber">
                <attribute name="ClientData" type="string"/>
                <anyAttribute processContents="lax"/>
            </extension>
        </simpleContent>
    </complexType>
    <!-- abstract so no reason to annotate -->
    <complexType name="ActionRecord" abstract="true">
        <sequence>
            <element name="Result" type="tns:Result"/>
            <element name="FinishTime" type="dateTime">
                <annotation>
                    <documentation>
                        <ann:summary>DateTime action finished in 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]' format
                        </ann:summary>
                        <ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="BilledAmount" type="float">
                <annotation>
                    <documentation>
                        <ann:summary>Amount charged for call or text</ann:summary>
                        <ann:demoValue>0.05</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="QuestionResponse" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <documentation>
                        <ann:summary>Question / Response pair defined in IVR campaign with &lt;a
                            href="https://www.callfire.com/help/docs/callfire-xml/tags/stash"&gt;'stash'.&lt;/a&gt;</ann:summary>
                    </documentation>
                </annotation>
                <complexType>
                    <sequence>
                        <element name="Question" type="string">
                            <annotation>
                                <documentation>
                                    <ann:summary>IVR stash 'varname'</ann:summary>
                                    <ann:demoValue>question1</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="Response" type="string">
                            <annotation>
                                <documentation>
                                    <ann:summary>IVR stash value associated with 'varname'</ann:summary>
                                    <ann:demoValue>1</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                    </sequence>
                </complexType>
            </element>
        </sequence>
        <attribute name="id" type="long" use="required"/>
    </complexType>
    <complexType name="Action">
        <sequence>
            <element name="FromNumber" type="tns:PhoneNumber">
                <annotation>
                    <documentation>
                        <ann:summary>E.164 11 digit number or short code</ann:summary>
                        <ann:summary>67076</ann:summary>
                    </documentation>
                </annotation>
            </element>
            <element name="ToNumber" type="tns:ToNumber">
                <annotation>
                    <documentation>
                        <ann:summary>List of E.164 11 digit numbers space seperated</ann:summary>
                        <ann:demoValue>13105551212 18185551212</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="State" type="tns:ActionState">
                <annotation>
                    <documentation>
                        <ann:summary>Current State of Action</ann:summary>
                        <ann:demoValue>FINISHED</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="BatchId" type="long" minOccurs="0">
                <annotation>
                    <documentation>
                        <ann:summary>Unique ID of Batch associated with Action</ann:summary>
                        <ann:demoValue>123</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="BroadcastId" type="long" minOccurs="0">
                <annotation>
                    <documentation>
                        <ann:summary>Unique ID of Broadcast associated with Action</ann:summary>
                        <ann:demoValue>123</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="ContactId" type="long">
                <annotation>
                    <documentation>
                        <ann:summary>Unique ID of Contact associated with Action</ann:summary>
                        <ann:demoValue>123</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="Inbound" type="boolean">
                <annotation>
                    <documentation>
                        <ann:summary>Is inbound Action</ann:summary>
                        <ann:demoValue>false</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="Created" type="dateTime">
                <annotation>
                    <documentation>
                        <ann:summary>DateTime Action was created 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
                        <ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="Modified" type="dateTime">
                <annotation>
                    <documentation>
                        <ann:summary>DateTime Action was modified 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
                        <ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
            <element name="FinalResult" type="tns:Result" minOccurs="0">
                <annotation>
                    <documentation>
                        <ann:summary/>
                        <ann:demoValue>NO_ANS</ann:demoValue>
                    </documentation>
                </annotation>
            </element>
        </sequence>
        <attribute name="id" type="long" use="required">
            <annotation>
                <documentation>
                    <ann:summary>Unique ID of action</ann:summary>
                    <ann:demoValue>123</ann:demoValue>
                </documentation>
            </annotation>
        </attribute>
    </complexType>
    <!-- Broadcast Elements -->
    <element name="Broadcast">
        <annotation>
            <documentation>
                <ann:summary>A TEXT, IVR, or VOICE Broadcast</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Name" type="string" minOccurs="0">
                    <annotation ann:requiredIn="POST">
                        <documentation>
                            <ann:summary>Name of Broadcast</ann:summary>
                            <ann:demoValue>My Test Broadcast</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Status" type="tns:BroadcastStatus" minOccurs="0">
                    <annotation ann:responseOnly="true">
                        <documentation>
                            <ann:summary>Status of Broadcast (response only)</ann:summary>
                        </documentation>
                    </annotation>
                </element>
                <element name="LastModified" type="dateTime" minOccurs="0">
                    <annotation ann:responseOnly="true">
                        <documentation>
                            <ann:summary>Last Modified</ann:summary>
                        </documentation>
                    </annotation>
                </element>
                <element name="Type" type="tns:BroadcastType" minOccurs="0">
                    <annotation ann:requiredIn="POST">
                        <documentation>
                            <ann:summary>Type of Broadcast (required for create)</ann:summary>
                            <ann:demoValue>TEXT</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <choice>
                    <element ref="tns:VoiceBroadcastConfig">
                        <annotation>
                            <documentation>
                                <ann:condition field="Type" value="VOICE"/>
                                <ann:summary>Configuration of a Voice Broadcast</ann:summary>
                            </documentation>
                        </annotation>
                    </element>
                    <element ref="tns:TextBroadcastConfig">
                        <annotation>
                            <documentation>
                                <ann:condition field="Type" value="TEXT"/>
                                <ann:summary>Configuration of a Text Broadcast</ann:summary>
                            </documentation>
                        </annotation>
                    </element>
                    <element ref="tns:IvrBroadcastConfig">
                        <annotation>
                            <documentation>
                                <ann:condition field="Type" value="IVR"/>
                                <ann:summary>Configuration of an IVR Broadcast</ann:summary>
                            </documentation>
                        </annotation>
                    </element>
                </choice>
            </sequence>
            <attribute name="id" type="long">
                <annotation>
                    <documentation>
                        <ann:summary>Unique ID of Broadcast</ann:summary>
                        <ann:demoValue>1</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
        </complexType>
    </element>
    <element name="VoiceBroadcastConfig">
        <annotation>
            <documentation>
                <ann:summary>Configuration needed for a Voice Broadcast</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <complexContent>
                <extension base="tns:BroadcastConfig">
                    <sequence>
                        <element name="AnsweringMachineConfig" type="tns:AnsweringMachineConfig" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>Action to take if machine answers</ann:summary>
                                    <ann:demoValue>LIVE_IMMEDIATE</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="LiveSoundId" type="long" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>ID of Sound to play if call answered by live person</ann:summary>
                                    <ann:demoValue>123</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="MachineSoundId" type="long" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>ID of Sound to play if call answered by machine</ann:summary>
                                    <ann:demoValue>124</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="TransferSoundId" type="long" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>ID of Sound to play if call transfered</ann:summary>
                                    <ann:demoValue>125</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="TransferDigit" type="tns:PhoneDigit" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>Phone digit call transfers on if pressed</ann:summary>
                                    <ann:demoValue>1</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="TransferNumber" type="tns:PhoneNumber" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>Number to transfer call to</ann:summary>
                                    <ann:demoValue>18185551212</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="DncSoundId" type="long" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>Do Not Call unique ID of sound</ann:summary>
                                    <ann:demoValue>12345</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="DncDigit" type="tns:PhoneDigit" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>Do Not Call Digit</ann:summary>
                                    <ann:demoValue>9</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="MaxActiveTransfers" type="int" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>Max Transfers</ann:summary>
                                    <ann:demoValue>1</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="TextBroadcastConfig">
        <annotation>
            <documentation>
                <ann:summary>Configuration needed for a Text Broadcast</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <complexContent>
                <extension base="tns:BroadcastConfig">
                    <sequence>
                        <element name="Message" type="string" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>160 char or less message to be sent in text broadcast. Use rented
                                        'keyword' in message if need response
                                    </ann:summary>
                                    <ann:demoValue>use your phone to text YOUR_KEYWORD to 67076, and we'll send you
                                        special coupons that you can use in our store.
                                    </ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element name="BigMessageStrategy" type="tns:BigMessageStrategy" minOccurs="0"
                                 default="SEND_MULTIPLE">
                            <annotation>
                                <appinfo>
                                    <jaxb:property generateIsSetMethod="true"/>
                                </appinfo>
                                <documentation>
                                    <ann:summary>Set strategy if message is over 160 chars (default: SEND_MULTIPLE)
                                    </ann:summary>
                                    <ann:demoValue>SEND_MULTIPLE</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="IvrBroadcastConfig">
        <annotation>
            <documentation>
                <ann:summary>Configuration needed for an IVR Broadcast</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <complexContent>
                <extension base="tns:BroadcastConfig">
                    <sequence>
                        <element name="DialplanXml" type="string" minOccurs="0">
                            <annotation>
                                <documentation>
                                    <ann:summary>IVR xml document describing dialplan</ann:summary>
                                </documentation>
                            </annotation>
                        </element>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="CallTrackingConfig">
        <complexType>
            <complexContent>
                <extension base="tns:InboundConfig">
                    <sequence>
                        <element name="TransferNumber" minOccurs="0">
                            <simpleType>
                                <list itemType="tns:PhoneNumber"/>
                            </simpleType>
                        </element>
                        <element name="Screen" type="boolean" minOccurs="0" default="false">
                            <annotation>
                                <appinfo>
                                    <jaxb:property generateIsSetMethod="true"/>
                                </appinfo>
                            </annotation>
                        </element>
                        <element name="Record" type="boolean" minOccurs="0" default="false">
                            <annotation>
                                <appinfo>
                                    <jaxb:property generateIsSetMethod="true"/>
                                </appinfo>
                            </annotation>
                        </element>
                        <element name="IntroSoundId" type="long" minOccurs="0"/>
                        <element name="WhisperSoundId" type="long" minOccurs="0"/>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="IvrInboundConfig">
        <complexType>
            <complexContent>
                <extension base="tns:InboundConfig">
                    <sequence>
                        <element name="DialplanXml" type="string" minOccurs="0"/>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="LocalTimeZoneRestriction">
        <annotation>
            <documentation>
                <ann:summary>Restrict the times your compaign can run</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="BeginTime" type="time" minOccurs="0">
                    <annotation>
                        <xs:appinfo xmlns:xs="http://www.w3.org/2001/XMLSchema"
                                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                            <jaxb:bindings node="ancestor::xs:annotation/..">
                                <ann:annotate ann:restParam="LocalRestrictBegin"/>
                            </jaxb:bindings>
                        </xs:appinfo>
                    </annotation>
                </element>
                <element name="EndTime" type="time" minOccurs="0">
                    <annotation>
                        <xs:appinfo xmlns:xs="http://www.w3.org/2001/XMLSchema"
                                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                            <jaxb:bindings node="ancestor::xs:annotation/..">
                                <ann:annotate ann:restParam="LocalRestrictEnd"/>
                            </jaxb:bindings>
                        </xs:appinfo>
                    </annotation>
                </element>
            </sequence>
        </complexType>
    </element>
    <element name="ContactBatch">
        <complexType>
            <sequence>
                <element name="Name" type="string">
                    <annotation>
                        <documentation>
                            <ann:summary>Name of Contact Batch</ann:summary>
                            <ann:demoValue>My TestContact Batch</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Status" type="tns:BatchStatus"/>
                <element name="BroadcastId" type="long">
                    <annotation>
                        <documentation>
                            <ann:summary>ID of Broadcast this Batch is attached to</ann:summary>
                            <ann:demoValue>3</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Created" type="dateTime"/>
                <element name="Size" type="int" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Contact count in this Batch</ann:summary>
                            <ann:demoValue>3</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Remaining" type="int" minOccurs="0"/>
            </sequence>
            <attribute name="id" type="long" use="required"/>
        </complexType>
    </element>
    <element name="BroadcastSchedule">
        <complexType>
            <sequence>
                <element name="StartTimeOfDay" type="time">
                    <annotation>
                        <documentation>
                            <ann:summary>Earliest time a client can be contacted in the timezone associated with the
                                number's NPA/NXX
                            </ann:summary>
                            <ann:demoValue>09:00:00</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="StopTimeOfDay" type="time">
                    <annotation>
                        <documentation>
                            <ann:summary>Latest time a client can be contacted in the timezone associated with the
                                number's NPA/NXX
                            </ann:summary>
                            <ann:demoValue>17:00:00</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="TimeZone" type="string">
                    <annotation>
                        <documentation>
                            <ann:summary>Time Zone</ann:summary>
                            <ann:demoValue>PST</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="BeginDate" type="date" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Start date of Campaign</ann:summary>
                            <ann:demoValue>2012-01-01</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="EndDate" type="date" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>End date of Campaign</ann:summary>
                            <ann:demoValue>2014-01-01</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="DaysOfWeek">
                    <simpleType>
                        <list itemType="tns:DayOfWeek"/>
                    </simpleType>
                </element>
            </sequence>
            <attribute name="id" type="long"/>
        </complexType>
    </element>
    <element name="BroadcastStats">
        <complexType>
            <sequence>
                <element name="UsageStats">
                    <complexType>
                        <sequence>
                            <element name="Duration" type="int">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Duration of calls in seconds</ann:summary>
                                        <ann:demoValue>2033</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="BilledDuration" type="int">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Duration of calls billed in seconds</ann:summary>
                                        <ann:demoValue>2033</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="BilledAmount" type="float">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Billed amount</ann:summary>
                                        <ann:demoValue>93.27</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="Attempts" type="int">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Attempted Texts and Calls</ann:summary>
                                        <ann:demoValue>432</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="Actions" type="int">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Text and Calls placed</ann:summary>
                                        <ann:demoValue>321</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </complexType>
                </element>
                <element name="ResultStat" minOccurs="0" maxOccurs="unbounded">
                    <complexType>
                        <sequence>
                            <element name="Result" type="tns:Result"/>
                            <element name="Attempts" type="int">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Text and Calls attempted</ann:summary>
                                        <ann:demoValue>20</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="Actions" type="int">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Text and Calls placed</ann:summary>
                                        <ann:demoValue>20</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </complexType>
                </element>
            </sequence>
        </complexType>
    </element>
    <!-- Call Elements -->
    <element name="CallRecord">
        <complexType>
            <complexContent>
                <extension base="tns:ActionRecord">
                    <sequence>
                        <element name="OriginateTime" type="dateTime" minOccurs="0"/>
                        <element name="AnswerTime" type="dateTime" minOccurs="0"/>
                        <element name="Duration" type="int">
                            <annotation>
                                <documentation>
                                    <ann:summary>Duration of call in seconds</ann:summary>
                                    <ann:demoValue>25</ann:demoValue>
                                </documentation>
                            </annotation>
                        </element>
                        <element ref="tns:RecordingMeta" minOccurs="0" maxOccurs="unbounded"/>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="Call">
        <annotation>
            <documentation>
                <ann:summary>Call Information</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <complexContent>
                <extension base="tns:Action">
                    <sequence>
                        <element ref="tns:CallRecord" minOccurs="0" maxOccurs="unbounded"/>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="SoundMeta">
        <annotation>
            <documentation>
                <ann:summary>Sounds are prerecorded audio stored on CallFire, and made available for use in voice
                    broadcasts and IVRs.
                </ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Status" type="tns:SoundStatus"/>
                <element name="Name" type="string">
                    <annotation>
                        <documentation>
                            <ann:summary>Name of sound meta</ann:summary>
                        </documentation>
                    </annotation>
                </element>
                <element name="Created" type="dateTime">
                    <annotation>
                        <documentation>
                            <ann:summary>DateTime when sound was created</ann:summary>
                        </documentation>
                    </annotation>
                </element>
                <element name="LengthInSeconds" type="int" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Duration of stored sound in seconds</ann:summary>
                        </documentation>
                    </annotation>
                </element>
            </sequence>
            <attribute name="id" type="long" use="required"/>
        </complexType>
    </element>
    <element name="RecordingMeta">
        <annotation>
            <documentation>
                <ann:summary>Recordings are audio recorded during a call.</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Name" type="string"/>
                <element name="Created" type="dateTime"/>
                <element name="LengthInSeconds" type="int" minOccurs="0"/>
            </sequence>
            <attribute name="id" type="long" use="required"/>
        </complexType>
    </element>
    <!-- Text Elements -->
    <element name="TextRecord">
        <complexType>
            <complexContent>
                <extension base="tns:ActionRecord">
                    <sequence>
                        <element name="Message" type="string"/>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="Text">
        <complexType>
            <complexContent>
                <extension base="tns:Action">
                    <sequence>
                        <element name="Message" type="string" minOccurs="0"/>
                        <element ref="tns:TextRecord" minOccurs="0" maxOccurs="unbounded"/>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </element>
    <element name="AutoReply">
        <complexType>
            <sequence>
                <choice>
                    <element name="Number" type="tns:PhoneNumber"/>
                    <element name="Keyword" type="string"/>
                </choice>
                <element name="Match" type="string"/>
                <element name="Message" type="string"/>
            </sequence>
            <attribute name="id" type="long"/>
        </complexType>
    </element>
    <!-- Subscription Elements -->
    <element name="Subscription">
        <annotation>
            <documentation>
                <ann:summary>Subscribe to a CallFire postback notification event, such as 'textnotification', by
                    registering a callback URI endpoint where events will get published to.
                </ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Enabled" type="boolean" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Enable publishing of postback notifications for subscribed events.
                            </ann:summary>
                            <ann:demoValue>true</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Endpoint" type="anyURI" minOccurs="0">
                    <annotation ann:requiredIn="POST">
                        <documentation>
                            <ann:summary>Email or URI endpoint to publish notification event to.</ann:summary>
                            <ann:demoValue>http://www.yourwebsite.com/somephpfile.php</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="NotificationFormat" type="tns:NotificationFormat" default="XML" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:demoValue>XML</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="TriggerEvent" type="tns:SubscriptionTriggerEvent" minOccurs="0">
                    <annotation ann:requiredIn="POST" ann:sinceVersion="1.1">
                        <documentation>
                            <ann:summary>Event to trigger on</ann:summary>
                            <ann:demoValue>CAMPAIGN_FINISHED</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="SubscriptionFilter" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Filter the type of notification events published to postback URI endpoint.
                            </ann:summary>
                        </documentation>
                    </annotation>
                    <complexType>
                        <sequence>
                            <element name="BroadcastId" type="long" minOccurs="0">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Broadcast ID to filter on</ann:summary>
                                        <ann:demoValue>123</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="BatchId" type="long" minOccurs="0">
                                <annotation>
                                    <documentation>
                                        <ann:summary>Batch ID to filter on</ann:summary>
                                        <ann:demoValue>123</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="FromNumber" type="tns:PhoneNumber" minOccurs="0">
                                <annotation>
                                    <documentation>
                                        <ann:summary>From number (11 digit) or shortcode to filter on</ann:summary>
                                        <ann:demoValue>123</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="ToNumber" type="tns:PhoneNumber" minOccurs="0">
                                <annotation>
                                    <documentation>
                                        <ann:summary>E.164 11 digit phone number</ann:summary>
                                        <ann:demoValue>13105551212</ann:demoValue>
                                    </documentation>
                                </annotation>
                            </element>
                            <element name="Inbound" type="boolean" minOccurs="0">
                                <annotation ann:deprecatedVersion="1.1">
                                    <documentation>
                                        <ann:summary>Deprecated, use TriggerEvent</ann:summary>
                                    </documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </complexType>
                </element>
            </sequence>
            <attribute name="id" type="long">
                <annotation>
                    <documentation>
                        <ann:summary>Unique ID of Subscription</ann:summary>
                        <ann:demoValue>123</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
        </complexType>
    </element>
    <!-- Contact Elements -->
    <element name="Contact">
        <annotation>
            <documentation>
                <ann:summary>Info about the people you want to contact.</ann:summary>
                <ann:description>Any info needed can be stored under Contact as an extra attribute.</ann:description>
            </documentation>
        </annotation>
        <complexType>
            <attribute name="id" type="long">
                <annotation>
                    <documentation>
                        <ann:summary>Unique ID of Contact</ann:summary>
                        <ann:demoValue>123</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
            <attribute name="firstName" type="string">
                <annotation>
                    <documentation>
                        <ann:summary>First name</ann:summary>
                        <ann:demoValue>John</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
            <attribute name="lastName" type="string">
                <annotation>
                    <documentation>
                        <ann:summary>Last name</ann:summary>
                        <ann:demoValue>Doe</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
            <attribute name="zipcode" type="string">
                <annotation>
                    <documentation>
                        <ann:summary>5 digit zipcode</ann:summary>
                        <ann:demoValue>98154</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
            <attribute name="homePhone" type="tns:PhoneNumber">
                <annotation>
                    <documentation>
                        <ann:summary>E.164 11 digit number</ann:summary>
                        <ann:demoValue>18185551212</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
            <attribute name="workPhone" type="tns:PhoneNumber">
                <annotation>
                    <documentation>
                        <ann:summary>E.164 11 digit number</ann:summary>
                        <ann:demoValue>18185551313</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
            <attribute name="mobilePhone" type="tns:PhoneNumber">
                <annotation>
                    <documentation>
                        <ann:summary>E.164 11 digit number</ann:summary>
                        <ann:demoValue>18185551414</ann:demoValue>
                    </documentation>
                </annotation>
            </attribute>
            <anyAttribute processContents="lax"/>
        </complexType>
    </element>
    <element name="ContactHistory">
        <annotation>
            <documentation>
                <ann:summary>List Calls or Texts associated with Contact</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <choice minOccurs="0" maxOccurs="unbounded">
                <element ref="tns:Call"/>
                <element ref="tns:Text"/>
            </choice>
        </complexType>
    </element>
    <element name="ContactList">
        <annotation>
            <documentation>
                <ann:summary>List of Contacts</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Name" type="string"/>
                <element name="Size" type="integer"/>
                <element name="Created" type="dateTime"/>
            </sequence>
            <attribute name="id" type="long" use="required"/>
        </complexType>
    </element>
    <!-- Number Elements -->
    <element name="Region">
        <annotation>
            <documentation>
                <ann:summary>Region of number represented by city, state, prefix, etc...</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Prefix" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>4-7 digit prefix</ann:summary>
                            <ann:demoValue>1920659</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="City" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Name of a city</ann:summary>
                            <ann:demoValue>Seattle</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="State" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>State abbreviation</ann:summary>
                            <ann:demoValue>WA</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Zipcode" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>5 digit zipcode</ann:summary>
                            <ann:demoValue>98154</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Country" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>2 digit country code</ann:summary>
                            <ann:demoValue>US</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Lata" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Lata</ann:summary>
                            <ann:demoValue>674</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="RateCenter" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:demoValue>SEATTLE</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Latitude" type="float" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Latitude</ann:summary>
                            <ann:demoValue>47.61</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="Longitude" type="float" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Longitude</ann:summary>
                            <ann:demoValue>-122.331</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="TimeZone" type="string" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:demoValue>America/Los_Angeles</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
            </sequence>
        </complexType>
    </element>
    <element name="LeaseInfo">
        <annotation>
            <documentation>
                <ann:summary>Info about Lease</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="LeaseBegin" type="date" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Date lease began</ann:summary>
                        </documentation>
                    </annotation>
                </element>
                <element name="LeaseEnd" type="date" minOccurs="0">
                    <annotation>
                        <documentation>
                            <ann:summary>Date lease ends</ann:summary>
                        </documentation>
                    </annotation>
                </element>
                <element name="AutoRenew" type="boolean">
                    <annotation>
                        <documentation>
                            <ann:summary>Is lease set to renew each month</ann:summary>
                        </documentation>
                    </annotation>
                </element>
            </sequence>
        </complexType>
    </element>
    <element name="Number">
        <annotation>
            <documentation>
                <ann:summary>Info about Number like Region</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Number" type="tns:PhoneNumber">
                    <annotation>
                        <documentation>
                            <ann:summary>11 digit E.164 number</ann:summary>
                            <ann:demoValue>13105551212</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="NationalFormat" type="string">
                    <annotation>
                        <documentation>
                            <ann:summary>Phone number formatted for local</ann:summary>
                            <ann:demoValue>(310) 555-1212</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element name="TollFree" type="boolean">
                    <annotation>
                        <documentation>
                            <ann:summary>Is toll free number</ann:summary>
                            <ann:demoValue>false</ann:demoValue>
                        </documentation>
                    </annotation>
                </element>
                <element ref="tns:Region" minOccurs="0"/>
                <!-- populated once purchased -->
                <element name="Status" type="tns:NumberStatus" minOccurs="0"/>
                <element ref="tns:LeaseInfo" minOccurs="0"/>
                <element ref="tns:NumberConfiguration" minOccurs="0"/>
            </sequence>
        </complexType>
    </element>
    <element name="NumberConfiguration">
        <annotation>
            <documentation>
                <ann:summary>Configure Call and Text features</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="CallFeature" type="tns:NumberFeature" minOccurs="0"/>
                <element name="TextFeature" type="tns:NumberFeature" minOccurs="0"/>
                <element name="InboundCallConfigurationType" type="tns:InboundType" minOccurs="0"/>
                <element name="InboundCallConfiguration" minOccurs="0">
                    <complexType>
                        <choice>
                            <element ref="tns:CallTrackingConfig"/>
                            <element ref="tns:IvrInboundConfig"/>
                        </choice>
                    </complexType>
                </element>
            </sequence>
        </complexType>
    </element>
    <element name="Keyword">
        <annotation>
            <documentation>
                <ann:summary>Info on Keyword like status and lease info</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="ShortCode" type="tns:PhoneNumber"/>
                <element name="Keyword" type="string"/>
                <!-- populated once purchased -->
                <element name="Status" type="tns:NumberStatus" minOccurs="0"/>
                <element ref="tns:LeaseInfo" minOccurs="0"/>
            </sequence>
        </complexType>
    </element>
    <complexType name="NumberOrderItem">
        <sequence>
            <element name="Ordered" type="int"/>
            <element name="UnitCost" type="float"/>
            <element name="Fulfilled" minOccurs="0">
                <simpleType>
                    <list itemType="string"/>
                </simpleType>
            </element>
        </sequence>
    </complexType>
    <element name="NumberOrder">
        <annotation>
            <documentation>
                <ann:summary>Info on NumberOrder like id, status, and cost</ann:summary>
            </documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="Status" type="tns:OrderStatus"/>
                <element name="Created" type="dateTime"/>
                <element name="TotalCost" type="float"/>
                <element name="LocalNumbers" type="tns:NumberOrderItem" minOccurs="0"/>
                <element name="TollFreeNumbers" type="tns:NumberOrderItem" minOccurs="0"/>
                <element name="Keywords" type="tns:NumberOrderItem" minOccurs="0"/>
            </sequence>
            <attribute name="id" type="long" use="required"/>
        </complexType>
    </element>
    <element name="Label">
        <complexType>
            <sequence>
                <element name="Name" type="string"/>
            </sequence>
        </complexType>
    </element>
</schema>